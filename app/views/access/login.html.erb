<% @page_title = "Admin login" %>
<br >
<br >
<br >
<div class="login">
  <%= form_tag(access_attempt_login_path, :method => :post) do %>
    <table>
      <tr>
        <td><%= label_tag(:username, "إسم المستخدم") %></td>
        <td><%= text_field_tag(:username, params[:username]) %></td>
      </tr>
      <tr>
        <td><%= label_tag(:password, "كلمة السر") %></td>
        <td><%= password_field_tag(:password) %></td>
      </tr>
      <tr>
        <td>&nbsp;</td>
        <td><%= submit_tag("دخول") %></td>
      </tr>
    </table>
  <% end %>
</div>
